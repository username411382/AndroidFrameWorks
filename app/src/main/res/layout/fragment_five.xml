<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:id="@+id/tv_video"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:background="@drawable/selector_five_page_text_view"
        android:gravity="center"
        android:text="视频"
        android:textColor="@color/white" />


    <TextView
        android:id="@+id/tv_shopping_car"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/selector_five_page_text_view"
        android:gravity="center"
        android:text="购物车"
        android:textColor="@color/white" />

    <TextView
        android:id="@+id/tv_sort"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:background="@drawable/selector_five_page_text_view"
        android:gravity="center"
        android:text="联系人按\n字母排序"
        android:textColor="@color/white" />

    <TextView
        android:id="@+id/tv_custom_pull_to_refresh"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:background="@drawable/selector_five_page_text_view"
        android:gravity="center"
        android:text="自定义\n下拉刷新"
        android:textColor="@color/white" />

    <TextView
        android:id="@+id/tv_first_user"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:background="@drawable/selector_five_page_text_view"
        android:gravity="center"
        android:text="用户一"
        android:textColor="@color/white" />

    <TextView
        android:id="@+id/tv_second_user"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:background="@drawable/selector_five_page_text_view"
        android:gravity="center"
        android:text="用户二"
        android:textColor="@color/white" />

</LinearLayout>
